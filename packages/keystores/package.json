{
  "name": "@meer-js/keystores",
  "version": "6.6.6",
  "description": "Key storage and management implementations",
  "types": "./dist/esm/keystores/src/index.d.ts",
  "main": "./dist/cjs/keystores/src/index.js",
  "module": "./dist/esm/keystores/src/index.js",
  "type": "module",
  "scripts": {
    "build": "yarn compile:esm",
    "postbuild": "yarn compile:cjs",
    "compile:esm": "tsc -p tsconfig.esm.json",
    "compile:cjs": "tsc -p tsconfig.cjs.json",
    "lint": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc",
    "lint:fix": "eslint -c ../../.eslintrc.ts.yml src/**/*.ts test/**/*.ts --no-eslintrc --fix",
    "test": "jest",
    "clean": "yarn rimraf dist"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@meer-js/crypto": "workspace:*",
    "@meer-js/types": "workspace:*",
    "@meer-js/utils": "workspace:*"
  },
  "devDependencies": {
    "@jest/globals": "^29.7.0",
    "@types/node": "^22.7.4",
    "jest": "29.7.0",
    "rimraf": "6.0.1",
    "ts-jest": "29.1.5",
    "typescript": "*"
  },
  "files": [
    "./dist/esm",
    "./dist/cjs"
  ],
  "exports": {
    ".": {
      "require": "./dist/cjs/keystores/src/index.js",
      "import": "./dist/esm/keystores/src/index.js"
    }
  }
}
