{
    "name": "near-api-js",
    "description": "JavaScript library to interact with NEAR Protocol via RPC API",
    "version": "5.0.0",
    "types": "./dist/esm/near-api-js/src/index.d.ts",
    "main": "./dist/cjs/near-api-js/src/index.js",
    "module": "./dist/esm/near-api-js/src/index.js",
    "type": "module",
    "scripts": {
        "build": "yarn compile:esm",
        "postbuild": "yarn compile:cjs",
        "compile:esm": "tsc -p tsconfig.esm.json",
        "compile:cjs": "tsc -p tsconfig.cjs.json",
        "test": "jest test --passWithNoTests",
        "lint:src": "eslint --ext .ts src",
        "lint:src:fix": "eslint --ext .ts --fix src",
        "lint:test:fix": "eslint --ext .js --fix test",
        "fuzz": "jsfuzz test/fuzz/borsh-roundtrip.js test/fuzz/corpus/",
        "clean": "yarn rimraf dist"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/near/near-api-js.git"
    },
    "homepage": "https://github.com/near/near-api-js",
    "browser": "./dist/esm/near-api-js/src/browser-index.js",
    "dependencies": {
        "@near-js/accounts": "workspace:*",
        "@near-js/crypto": "workspace:*",
        "@near-js/keystores": "workspace:*",
        "@near-js/keystores-browser": "workspace:*",
        "@near-js/keystores-node": "workspace:*",
        "@near-js/providers": "workspace:*",
        "@near-js/signers": "workspace:*",
        "@near-js/transactions": "workspace:*",
        "@near-js/types": "workspace:*",
        "@near-js/utils": "workspace:*",
        "@near-js/wallet-account": "workspace:*",
        "@noble/curves": "1.2.0",
        "borsh": "1.0.0",
        "depd": "2.0.0",
        "http-errors": "1.7.2",
        "near-abi": "0.1.1",
        "node-fetch": "2.6.7"
    },
    "devDependencies": {
        "@types/http-errors": "1.6.1",
        "@types/node": "^22.7.4",
        "bs58": "4.0.0",
        "concurrently": "7.3.0",
        "in-publish": "2.0.0",
        "jest": "29.7.0",
        "near-hello": "0.5.1",
        "near-workspaces": "3.5.0",
        "rimraf": "^6.0.1",
        "semver": "7.1.1",
        "ts-jest": "29.1.5",
        "typescript": "^5.6.2"
    },
    "keywords": [],
    "license": "(MIT AND Apache-2.0)",
    "files": [
        "./dist/esm",
        "./dist/cjs"
    ],
    "exports": {
        ".": {
            "require": "./dist/cjs/near-api-js/src/index.js",
            "import": "./dist/esm/near-api-js/src/index.js"
        }
    },
    "author": "NEAR Inc"
}
