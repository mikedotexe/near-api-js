{
  "name": "@near-js/wallet-account",
  "version": "1.3.0",
  "description": "Dependencies for the NEAR API JavaScript client in the browser",
  "types": "./dist/esm/wallet-account/src/index.d.ts",
  "main": "./dist/cjs/wallet-account/src/index.js",
